    "prebuild": "prebuildify --napi --strip",
    "install": "npm run build-addon"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "prebuild": "^12.0.0",
    "prebuildify": "^5.0.0",
    "prebuild-install": "^7.1.3"
  },
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.3.0"
  },
